{
  "name": "frontend",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "classnames": "^2.2.6",
    "date-fns": "^2.11.1",
    "faker": "^4.1.0",
    "jwt-decode": "^2.2.0",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-table": "^7.0.4",
    "typescript": "^3.7.5",
    "uswds": "^2.6.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/classnames": "^2.2.9",
    "@types/faker": "^4.1.11",
    "@types/jest": "^24.9.0",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^12.12.34",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-table": "^7.0.13",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "node-sass": "^4.13.1",
    "prettier": "1.19.1",
    "react-scripts": "3.4.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "typecheck": "tsc --noEmit",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test --watch",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ci": "react-scripts test --coverage --b --ci --watchAll=false",
    "lint": "eslint 'src/**/*.{ts,tsx,json,js,jsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx,json,js,jsx}' --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie >= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "lines": 80
      }
    },
    "collectCoverageFrom": [
      "./src/**/*.{ts,js,tsx,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "serviceWorker.ts",
      "react-app-env.d.ts",
      "src/index.tsx",
      "src/test-utils.tsx",
      "src/App.tsx",
      "src/components/.*index.ts",
      "src/solo-uswds/.*index.ts",
      "src/hooks/*.index.ts",
      "src/context/.*index.ts",
      "src/pages/.*index.ts",
      "src/components/.*fakeDoc.ts",
      "src/solo-types/.*.ts"
    ]
  }
}
